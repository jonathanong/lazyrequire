{
  "name": "lazyrequire",
  "description": "lazily require modules in node.js",
  "version": "1.3.0",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jonathanong/lazyrequire",
  "dependencies": {
    "callsite": "1",
    "chalk": "1",
    "fn-getter": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "4",
    "codecov": "1",
    "eslint": "1",
    "eslint-config-standard": "4",
    "eslint-plugin-standard": "1",
    "ghooks": "1",
    "nyc": "5",
    "mocha": "2"
  },
  "scripts": {
    "lint": "eslint --cache lib test",
    "test": "mocha test/test.js",
    "test-cov": "nyc node_modules/mocha/bin/_mocha test/test.js && nyc report --reporter=lcov"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "keywords": [
    "lazy",
    "require"
  ],
  "files": [
    "lib"
  ],
  "main": "lib"
}
