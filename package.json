{
  "name": "lazyrequire",
  "description": "lazily require modules in node.js",
  "version": "1.3.0",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jonathanong/lazyrequire",
  "dependencies": {
    "callsite": "^1.0.0",
    "chalk": "^1.1.1",
    "fn-getter": "^1.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0",
    "codecov": "^1.0.1",
    "eslint": "^2.4.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "ghooks": "^1.0.3",
    "mocha": "^2.4.5",
    "nyc": "^6.0.0"
  },
  "scripts": {
    "lint": "eslint --cache lib test",
    "test": "mocha test/test.js",
    "test-cov": "nyc node_modules/mocha/bin/_mocha test/test.js && nyc report --reporter=lcov"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run lint"
    }
  },
  "keywords": [
    "lazy",
    "require"
  ],
  "files": [
    "lib"
  ],
  "main": "lib"
}
